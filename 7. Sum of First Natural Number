/* Sum of First n Number */

import java.util.Scanner;
public class Main
{   
   public static void main(String[] args)
   {
       int n,sum=0;
       System.out.print("Enter the number");
       Scanner r=new Scanner(System.in);
       n=r.nextInt();
       
       for(int i=1;i<=n;i++)
       {
           sum=sum+i;
       }
       System.out.print("Addition"+ sum);
       }
   }
