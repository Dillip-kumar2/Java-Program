import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    int n;
	    System.out.print("Enter the number");
		Scanner r =new Scanner(System.in);
	    n=r.nextInt();
	    if(n%2==0)
	    {
	        System.out.print("Even number");
	    }
	    else  
	    {
	        System.out.print("Odd number");
	    }
	    
	}
	
}
