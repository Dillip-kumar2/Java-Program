/* Print Even Number in the given range*/
import java.util.Scanner;
public class Main
{   
   public static void main(String[] args)
   {
       int n;
       System.out.print("Enter the number");
       Scanner r=new Scanner(System.in);
       n=r.nextInt();
       
       for(int i=0;i<=n;i=i+2)
       {
        System.out.print(i + " ");
       }
   }
}
